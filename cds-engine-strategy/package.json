{
  "name": "@portfolio/cds-engine-strategy",
  "version": "1.0.0",
  "description": "Call Debit Spread (CDS) trading engine - finds optimal entry opportunities",
  "module": "src/index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "analyze": "bun run src/index.ts analyze",
    "scan": "bun run src/index.ts scan",
    "scan:sp500": "bun run src/index.ts scan --list sp500 --min-score 80",
    "scan:test": "bun run src/index.ts scan --tickers NVDA,AAPL,MSFT,CRWD,HOOD --dry-run --min-score 10",
    "scan-spreads": "bun run src/index.ts scan-spreads",
    "scan-all": "bun run src/index.ts scan-all",
    "scan-watchlist": "bun run src/index.ts scan-all --watchlist",
    "scan-db": "bun run src/index.ts scan-all --from-db",
    "scan-universe": "bun run src/index.ts scan-all --from-tickers --db-limit 0",
    "scan-nasdaq": "bun run src/index.ts scan-all --from-tickers --exchange NASDAQ",
    "scan-nyse": "bun run src/index.ts scan-all --from-tickers --exchange NYSE",
    "engine": "bun run src/index.ts scan-all --summary",
    "regime": "bun run src/index.ts regime",
    "sectors": "bun run src/index.ts sectors",
    "watchlist": "bun run src/index.ts watchlist",
    "earnings": "bun run src/index.ts earnings",
    "briefing": "bun run src/index.ts briefing",
    "trade": "bun run src/index.ts trade",
    "performance": "bun run src/index.ts performance",
    "backtest": "bun run src/index.ts backtest",
    "trends": "bun run src/index.ts trends",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "test": "bun test"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.86.0",
    "chalk": "^5.6.2",
    "cheerio": "^1.1.2",
    "cli-table3": "^0.6.5",
    "commander": "^14.0.2",
    "dotenv": "^17.2.3",
    "ollama": "^0.6.3",
    "technicalindicators": "^3.1.0",
    "yahoo-finance2": "^3.10.2",
    "yaml": "^2.8.2",
    "zod": "^4.1.13"
  }
}
